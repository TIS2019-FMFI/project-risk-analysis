<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>
<AnchorPane layoutX="14.0" layoutY="4.0" prefHeight="43.0" prefWidth="571.0"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="app.gui.project.ProjectFilter">

    <children>
        <ImageView fx:id="backButton" onMouseClicked="#onBackPressed"  fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" >
            <image>
                <Image url="@/app/images/back.png" />
            </image>
        </ImageView>
        <DatePicker fx:id="fromDatepicker" layoutY="9.0" promptText="Od" AnchorPane.leftAnchor="50.0" />
        <DatePicker fx:id="toDatepicker" layoutX="193.0" layoutY="9.0" promptText="Do" AnchorPane.leftAnchor="230.0" />
        <ImageView onMouseClicked="#refreshFilter" fitHeight="30.0" fitWidth="40.0" layoutX="415.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@/app/images/refresh.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>